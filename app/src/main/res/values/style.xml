<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- Init -->
    <style name="init_tv_title">
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">#FFFFFF</item>
    </style>

    <!-- Home Style -->
    <style name="tv_guide_location">
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">#FFFFFF</item>
    </style>

    <style name="home_location">
        <item name="android:textSize">24sp</item>
        <item name="android:textColor">#FFFFFF</item>
    </style>

    <style name="home_content">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">#FFFFFF</item>
    </style>

    <!-- Commend Style -->
    <style name="commend_title">
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">@color/primary_01</item>
    </style>

    <style name="commend_tomorrow">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <!-- MyPage Style -->
    <style name="mypage_user_name">
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <!-- MyPage Style -->
    <style name="mypage_system_settings">
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <!--  ImageView Style  -->
    <style name="ShapeAppearanceOverlay.App.CornerSize50Percent" parent="">
        <item name="cornerSize">50%</item>
    </style>

    <style name="ShapeAppearanceOverlay.App.CornerSize15Percent" parent="">
        <item name="cornerSize">25%</item>
    </style>

    <style name="ShapeAppearanceOverlay.App.CornerSize10Percent" parent="">
        <item name="cornerSize">10%</item>
    </style>

    <!-- dialog -->
    <style name="MaterialAlertDialog.App" parent="MaterialAlertDialog.Material3">
        <item name="shapeAppearance">@style/ShapeAppearance.App.MediumComponent</item>
        <item name="shapeAppearanceOverlay">@null</item>
    </style>
    <style name="ShapeAppearance.App.MediumComponent" parent="ShapeAppearance.Material3.MediumComponent">
        <item name="cornerFamily">cut</item>
        <item name="cornerSize">8dp</item>
    </style>
</resources>